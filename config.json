{
  "name": "Export videos project in Supervisely format",
  "type": "app",
  "categories": [
    "videos",
    "export"
  ],
  "description": "Export videos project and prepares downloadable tar archive",
  "docker_image": "supervisely/base-py-sdk:6.35.0",
  "instance_version": "6.4.57",
  "main_script": "src/main.py",
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "items": true
  },
  "task_location": "workspace_tasks",
  "isolate": true,
  "headless": true,
  "icon": "https://user-images.githubusercontent.com/48913536/175979192-3b62a77c-15ac-4970-8767-e75c5864dd7d.png",
  "icon_cover": true,
  "icon_background": "#FFFFFF",
  "context_menu": {
    "target": [
      "videos_project"
    ],
    "context_root": "Download as"
  },
  "poster": "https://user-images.githubusercontent.com/48245050/182363095-45b662ce-c732-4cb0-929e-53316e99f883.png"
}
